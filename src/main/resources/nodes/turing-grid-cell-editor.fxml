<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<fx:root xmlns:fx="http://javafx.com/fxml/1"
         type="javafx.scene.layout.VBox"
         xmlns="http://javafx.com/javafx/8.0.172-ea">
    <children>
        <GridPane alignment="CENTER" hgap="10.0" vgap="5.0">
            <columnConstraints>
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
            </columnConstraints>
            <rowConstraints>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
            </rowConstraints>
            <children>
                <Label alignment="CENTER_RIGHT" contentDisplay="RIGHT" text="New Character" textAlignment="RIGHT"
                       GridPane.halignment="RIGHT" GridPane.valignment="CENTER"/>
                <Label alignment="CENTER_RIGHT" text="Movement" textAlignment="RIGHT" GridPane.halignment="RIGHT"
                       GridPane.rowIndex="1" GridPane.valignment="CENTER"/>
                <Label alignment="CENTER_RIGHT" text="NextState" textAlignment="RIGHT"
                       GridPane.halignment="RIGHT" GridPane.rowIndex="2" GridPane.valignment="CENTER"/>
                <ComboBox fx:id="characterComboBox" minWidth="50.0" GridPane.columnIndex="1"/>
                <ComboBox fx:id="pommelMovementComboBox" minWidth="50.0" GridPane.columnIndex="1"
                          GridPane.rowIndex="1"/>
                <ComboBox fx:id="nextStateComboBox" minWidth="50.0" GridPane.columnIndex="1" GridPane.rowIndex="2"/>
                <Button fx:id="saveButton" GridPane.columnIndex="1" GridPane.rowIndex="3" text="Save"/>
            </children>
            <padding>
                <Insets bottom="5.0" left="5.0" right="5.0" top="5.0"/>
            </padding>
        </GridPane>
    </children>
</fx:root>
